{
  "schemaVersion": 1,
  "id": "chatonlostfocus",
  "version": "${version}",
  "name": "ChatOnLostFocus",
  "description": "This mod replaces the default functionality of Minecraft to open chat instead of menu when the window is not focused.",
  "authors": ["atikiNBTW"],
  "contact": {
    "homepage": "https://modrinth.com/mod/chat-on-lost-focus",
    "sources": "https://github.com/atikiNBTW/ChatOnLostFocus"
  },
  "license": "The MIT License",
  "icon": "assets/chatonlostfocus/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.atikinbtw.chatOnLostFocus.ChatOnLostFocus"
    ],
    "modmenu": [
      "com.atikinbtw.chatOnLostFocus.config.ModMenuConfig"
    ]
  },
  "mixins": [
    "chatonlostfocus.mixins.json"
  ],
  "depends": {
    "fabricloader": "~0.15",
    "fabric": "*",
    "minecraft": "~1.20"
  }
